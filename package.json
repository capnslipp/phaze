{
  "name": "phaze",
  "version": "1.0.0",
  "description": "A phase vocoder for the browser",
  "main": "index.js",
  "scripts": {
    "build-main": "browserify main.js -o www/main.js",
    "build-audio-worklet": "browserify phase-vocoder.js -o www/phase-vocoder.js",
    "watch-main": "watchify main.js -o www/main.js --debug --verbose ",
    "watch-audio-worklet": "watchify phase-vocoder.js -o www/phase-vocoder.js --debug --verbose",
    "build": "npm run build-main | npm run build-audio-worklet",
    "watch": "npm run watch-main | npm run watch-audio-worklet",
    "start": "http-server www/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "phase",
    "vocoder",
    "pitch",
    "time",
    "stretch"
  ],
  "author": "Mano, Gon & Olivier",
  "license": "The Unlicense",
  "dependencies": {
    "fft.js": "^4.0.3",
    "http-server": "^0.12.1",
    "waves-audio": "^0.5.2",
    "waves-loaders": "^0.1.5",
    "waves-ui": "github:wavesjs/ui"
  },
  "devDependencies": {
    "browserify": "^16.5.0",
    "watchify": "^3.11.1"
  }
}
